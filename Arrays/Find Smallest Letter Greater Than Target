var nextGreatestLetter = function(letters, target) {

    let left = 0;
    let right = letters.length - 1;
    let ans = null;

    while (left <= right) {
        let mid = Math.floor((left + right) / 2);

        if (letters[mid] <= target) {
            left = mid + 1;
        } else {
            ans = letters[mid];
            right = mid - 1;
        }
    }

    return ans === null ? letters[0] : ans;
};
